{
  "expo": {
    "name": "awesome",
    "slug": "awesome",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/posindo.png",
    "version": "0.0.1",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.appawesome.cob",
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "notification":{
      "icon": "./assets/notification.png",
      "color": "#f68527",
      "androidMode": "default",
      "iosDisplayInForeground": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
